{
   "name": "quarantina",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start"
   },
   "dependencies": {
      "@google-cloud/dialogflow": "^3.4.0",
      "next": "12.1.0",
      "react": "17.0.2",
      "react-dom": "17.0.2",
      "react-is": "^16.8.0",
      "styled-components": "^5.2.3",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "babel-plugin-styled-components": "^1.12.0",
      "prettier": "^2.2.1"
   },
   "prettier": {
      "singleQuote": false,
      "printWidth": 100,
      "semi": true,
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "arrowParens": "avoid",
      "trailingComma": "all",
      "tabWidth": 3
   },
   "babel": {
      "env": {
         "development": {
            "plugins": [
               [
                  "styled-components",
                  {
                     "ssr": true,
                     "pure": true,
                     "displayName": true,
                     "preprocess": false
                  }
               ]
            ],
            "presets": [
               "next/babel"
            ]
         },
         "production": {
            "plugins": [
               [
                  "styled-components",
                  {
                     "ssr": true,
                     "pure": true,
                     "displayName": true,
                     "preprocess": false
                  }
               ]
            ],
            "presets": [
               "next/babel"
            ]
         }
      },
      "plugins": [
         [
            "styled-components",
            {
               "ssr": true,
               "pure": true,
               "displayName": true,
               "preprocess": false
            }
         ]
      ]
   }
}
